.listner_search_result
  .container_listener.list-group
    - if @listeners.any?
      - @listeners.each do |l| 
        %ul.col-sm-3.listner_summary.list-group-item
          %li name: #{l.name}
          %li language:  #{l.language}
          %li age group: #{l.agegroup}
          %li category: #{l.categories}
          %li license: #{l.license_type}
          %li religion: #{l.religion}
          %a.btn.btn-primary.btn-xs{:href => "/users/chat_session", :type => "button", :data => l.id } Start a Conversation
          %a.btn.btn-primary.btn-xs{:href => "users/#{l.id}", :type => "button", :data => l.id, } Detail
          